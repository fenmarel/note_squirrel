<div class="list-group-item" id="notebook-form-container"></div>

<a id="new-notebook-toggle" class="list-group-item" href="#">
  <span class="glyphicon glyphicon-plus"></span> New Notebook
</a>

<div class="list-group-item sidebar-title" id="notebooks-collapse">
  <span class="glyphicon glyphicon-chevron-down"></span> Workspace: <%= dashboard.escape('title') %>
  <span class="glyphicon glyphicon-pencil edit-dashboard-name pull-right"></span>
</div>

<div id="sidebar-notebook-workspace-name-form" class="list-group-item sidebar-title">
  <form id="rename-dashboard" accept-charset="utf-8">
    <input type="text" name="dashboard[title]" autofocus
           value="<%= dashboard.escape('title') %>"
           class="form-control dashboard-title input-sm">

    <span class="submit-options">
      <input type="submit" value="Update" class="btn btn-default btn-sm">
      <span id="untoggle-dashboard-form"
            class="glyphicon glyphicon-remove"></span>
    </span>
  </form>
</div>

<% notebooks.each(function(notebook) { %>
  <a class="list-group-item notebook-sidebar-item" href="#<%= notebook.url() %>">
    <span class="glyphicon glyphicon-book"></span>
    <%= notebook.escape('title') %>
  </a>
<% }); %>